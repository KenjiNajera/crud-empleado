<div class="form-group row">
    <label class="col-form-label col-sm-2" for="nombre">Nombre</label>
    <div class="col-sm-8">
      <input readonly autofocus class="form-control" name="nombre" value="<%= model.nombre %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="correo">Correo</label>
    <div class="col-sm-7">
      <input readonly class="form-control" name="correo" value="<%= model.correo %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="fechaIngreso">Fecha de ingreso</label>
    <div class="col-sm-10">
        <input readonly type="date" placeholder="YYYY-MM-DD" name="fechaIngreso" value="<%= model.fechaIngreso %>">
    </div>
  </div>