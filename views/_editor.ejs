<div class="form-group row">
  <label class="col-form-label col-sm-2" for="nombre">Nombre</label>
  <div class="col-sm-8">
    <input
      autofocus
      class="form-control"
      name="nombre"
      value="<%= model.nombre %>"
      required
    />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="apellido">apellido</label>
  <div class="col-sm-7">
    <input
      type="text"
      class="form-control"
      name="apellido"
      value="<%= model.apellido %>"
      required
    />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="correo">Correo</label>
  <div class="col-sm-7">
    <input
      type="email"
      class="form-control"
      name="correo"
      value="<%= model.correo %>"
      required
    />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="fechaIngreso"
    >Fecha de ingreso</label
  >
  <div class="col-sm-10">
    <input
      type="date"
      name="fechaIngreso"
      value="<%= new Date(model.fechaingreso).toISOString().slice(0, 10) %>"
      required
    />
  </div>
</div>
